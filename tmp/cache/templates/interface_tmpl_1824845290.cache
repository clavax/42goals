    <?php echo '<script'?> type="text/javascript">
      google.load("visualization", "1", {packages:["corechart"]});
      function drawChart() {
        var data = new google.visualization.DataTable();
        data.addColumn('string', 'Year');
        data.addColumn('number', 'Sales');
        data.addColumn('number', 'Expenses');
        data.addRows(4);
        data.setValue(0, 0, '2004');
        data.setValue(0, 1, 1000);
        data.setValue(0, 2, 400);
        data.setValue(1, 0, '2005');
        data.setValue(1, 1, 1170);
        data.setValue(1, 2, 460);
        data.setValue(2, 0, '2006');
        data.setValue(2, 1, 660);
        data.setValue(2, 2, 1120);
        data.setValue(3, 0, '2007');
        data.setValue(3, 1, 1030);
        data.setValue(3, 2, 540);

        var chart = new google.visualization.ColumnChart(document.getElementById('chart'));
        chart.draw(data, {height: 240, title: 'Company Performance',
          hAxis: {title: 'Year', titleTextStyle: {color: 'red'}}
         });
      }

document.addLoader(function () {
  
  window.close_sidebar = function () {
      ID('content').replaceClass('collapsed', 'expanded');
  }
  
  window.open_sidebar = function () {
      ID('content').replaceClass('expanded', 'collapsed');
      drawChart();
  }

  var r1 = new Raphael('pie');
  r1.g.piechart(30, 15, 15, [Math.rand(0, 30), Math.rand(0, 30)], {colors: ['#1f77b4', '#aec7e8']});
  
  var r2 = new Raphael('line');
  var x = [], y = [];
  for (var i = 0; i < 28; i ++) {
      x.push(i);
      y.push(Math.rand(0, 30));
  }
  r2.g.linechart(0, 0, 60, 30, x, y, {width: 1, gutter: 1e-5, vgutter: 0, colors: ['#1f77b4', '#aec7e8']});
  
  var r3 = new Raphael('bar');
  var d = [];
  for (var i = 0; i < 28; i ++) {
      d.push(Math.rand(0, 30));
  }
  r3.g.barchart(0, 0, 60, 30, [d], {type: 'sharp', gutter: 1e-5, vgutter: 0, colors: ['#1f77b4', '#aec7e8']});
  
  close_sidebar();
});

